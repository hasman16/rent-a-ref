  "scripts": {
    "ng": "ng",
    "build": "ng build --prod --aot",
    "start": "node dist/server/app.js",
    "predev": "tsc -p server",
    "back": "concurrently  \"tsc -w -p server\" \"nodemon dist/server/app.js\"",
    "dev": "concurrently  \"ng serve --aot --proxy-config proxy.conf.json --open\" \"tsc -w -p server\" \"nodemon dist/server/app.js\"",
    "prod": "concurrently \"ng build --aot --prod && tsc -p server && node dist/server/app.js\"",
    "test": "ng test",
    "testbe": "tsc -p server && mocha dist/server/test --exit",
    "lint": "ng lint",
    "lintbe": "tslint server/**/**.ts{,x}",
    "e2e": "ng e2e"
  },
